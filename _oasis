Library "local_libs"
   Path: src/libs
   Modules : Misc_library, Maps
   BuildDepends : batteries


Library "reactions"	
  FindLibName : reactions
  FindLibParent : bacterie
  Path: src/bacterie
  Modules: Reactions
  

Library "molecule"	
  FindLibName : molecule_mod
  FindLibParent : bacterie
  Path: src/bacterie
  Modules: Molecule_module
  BuildDepends: local_libs


Library "types"
  FindLibName : types
  FindLibParent : bacterie
  Path : src/bacterie
  Modules : Types
  BuildDepends : bacterie.molecule_mod


Library "proteine"
  FindLibName : proteine_mod
  FindLibParent : bacterie
  Path: src/bacterie
  Modules: Proteine_module
  BuildDepends: batteries, bacterie.molecule_mod, local_libs


